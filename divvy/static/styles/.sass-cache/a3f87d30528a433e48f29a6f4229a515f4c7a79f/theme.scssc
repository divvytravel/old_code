  box-shadow: 1px 0 5px rgba(22, 76, 89, 0.15);
  color: rgba(0, 0, 0, 0.9);
  font-size: 13px;
  height: 32px;
  // line-height: 25px;
  padding: 8px 8px;
  position: relative;
  text-align: left;
  vertical-align: top;
  border-radius: 3px;
  cursor: pointer;
  display: block;
  width: 100%;
  outline: none;
  &::-webkit-input-placeholder {
    color: #31B9DA;
  }
  &:hover,
  &:focus {
    background-color: rgba(49, 185, 218, 0.09);
    border: 1px solid rgba(49, 185, 218, 0.5);
  }
}

.date-picker-proj {
  position: relative;
  cursor: pointer;
  &:after {
    content: "";
    display: block;
    position: absolute;
    height: 15px;
    width: 17px;
    top: 8px;
    right: 8px;
    background-image: url("../img/calendar-blue.png");
    background-repeat: no-repeat;
    background-position: 0px 0px;
    cursor: pointer;
  }
}

.select-place-proj {
  position: relative;
  cursor: pointer;
  &:after {
    content: "";
    display: block;
    position: absolute;
    height: 15px;
    width: 17px;
    top: 14px;
    right: 5px;
    background-image: url("../img/arrow-down-medium.png");
    background-repeat: no-repeat;
    background-position: 0px 0px;
    cursor: pointer;
  }
}

.calendar-black,
.calendar-blue {
  display: inline-block;
  margin-right: 6px;
  width: 17px;
  height: 15px;
}
.calendar-black {
  background-image: url("/static/img/calendar-black.png");
}
.calendar-blue {
  background-image: url("/static/img/calendar-blue.png");
}


@-webkit-keyframes spinner {
  0% {
    -webkit-transform: rotate(0deg);
  }

  100% {
    -webkit-transform: rotate(360deg);
  }
}
.spinner {
  display: block;
  padding: 25px;
  text-align: center;
}
.spinner svg {
  display: inline-block;
  width: 50px;
  height: 50px;
  fill: #31b9da;
  -webkit-animation: spinner 2.2s infinite linear;
}

.tooltip-proj {

}

.tooltip-inner {
  max-width: $tooltip-max-width;
  width: 180px;
  padding: 8px 8px;
  color: $tooltip-color;
  text-align: center;
  text-decoration: none;
  background-color: $tooltip-bg;
  border-radius: $border-radius-base;
  font-size: 11px;
}

@import "parts/slider";
@import "parts/typeahead";
@import "parts/tooltip";
@import "parts/datepicker";
