@-webkit-keyframes spinner
    0%
      -webkit-transform: rotate(0deg)
    100%
      -webkit-transform: rotate(360deg)

.spinner
  display block
  padding 5px
  text-align center

  svg
    display inline-block
    width 32px
    height 32px
    fill #31b9da
    -webkit-animation spinner 2.2s infinite linear
